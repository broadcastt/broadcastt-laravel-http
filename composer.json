{
    "name": "broadcastt/broadcastt-laravel-http",
    "homepage": "https://broadcastt.xyz/",
    "description": "Laravel library for Broadcastt HTTP API",
    "keywords": ["broadcastt", "broadcast", "events", "laravel", "messaging", "publish", "push", "rest", "realtime", "real-time", "real time", "trigger"],
    "license": "MIT",
    "require": {
        "broadcastt/broadcastt-php-http": "^0.4.3",
        "illuminate/broadcasting": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "php": "^8.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Broadcastt\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Broadcastt\\Laravel\\BroadcasttServiceProvider"
            ],
            "aliases": {
                "Broadcastt": "Broadcastt\\Laravel\\Facades\\Broadcastt"
            }
        }
    },
    "prefer-stable": true
}
